{
    "manifest_version": 2,
   
    "name": "Yandex Play",
    "version": "0.1.0",
    "description": "Play music in your player",
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "*://*.google.com/",
        "https://music.yandex.ru/*",
        "https://*.storage.yandex.net/get-mp3/"
    ],
    "background": {
        "scripts": [
            "background.js"
        ]
    }
  }